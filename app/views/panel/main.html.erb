<div class="row">
  <div class="col-md-6 col-md-offset-3">
	<% User.all.each do |user| %>
      	<li><%= user.name %></li>
    	<%= form_for(user) do |f| %>
    		<%= f.check_box :trainer %>
    	<%= f.submit 'Submit!' %>
    	<%= link_to "Delete", user, method: :delete %>
    	<% end %>
    <% end %>
   </div>
</div>